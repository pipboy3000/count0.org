  "scripts": {
    "start": "bundle exec foreman start",
    "webpack:dev": "NODE_ENV=development webpack-dev-server --color --hot --inline --content-base ./dist/",
    "webpack:prod": "NODE_ENV=production webpack --color --profile --progress --hide-modules",
    "publish": "bundle exec jekyll build && npm run webpack:prod && npm run imagemin && bundle exec s3_website push",
    "imagemin": "mkdir -p dist/images;imagemin src/images/* --out-dir=dist/images"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.2.0",
    "lodash": "^4.17.11",
    "normalize.css": "^7.0.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "imagemin-cli": "^3.0.0",
    "node-sass": "^4.10.0",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^6.0.7",
    "sass-module-importer": "^1.2.1",
    "style-loader": "^0.18.2",
