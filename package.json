    "browserify": "^12.0.1",
    "clean-css": "^3.4.8",
    "coffee-script": "^1.8.0",
    "del": "^2.0.2",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-imagemin": "^2.3.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.3",
    "gulp-sourcemaps": "^1.5.2",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "run-sequence": "^1.1.1",
    "uglify-js": "^2.6.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
