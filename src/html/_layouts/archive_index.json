{
  {% for archive in page.archives %}
  "{{ archive[0] }}": [
    {% for post in archive[1]%}
    {
      "title": "{{ post.title }}",
      "url": "{{ post.url }}",
      "date": "{{ post.date | date_to_long_string }}",
      "excerpt": "{{ post.content | strip_html | xml_escape |truncate: 110 }}"
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
  {% endfor %}
}
